}

function AddClickEventListenerToUpcomingEvents(){
     var ulElement =
        document.querySelector("div.ContentUserControl.featured> .HLLandingControl.HLEventList > div.Content > div.row >div > ul")
    console.log('ulElement', ulElement)
    // Evaluate the XPath expression to get the ul element
    // var ulElement = document.evaluate(xpathExpression, document, null, XPathResult.FIRST_ORDERED_NODE_TYPE, null).singleNodeValue;

    if (ulElement) {
        // Get all li elements within the ul group
        var liElements = ulElement.querySelectorAll('li');

        // Attach the click event listener to each li element
        liElements.forEach(function (li) {
            var link = li.querySelector('a') || li.querySelector('div > div > h3 > a');
            if (link) {
                li.addEventListener('click', function () {
                    link.click();
                });

                // Prevent the event from propagating to nested elements
                link.addEventListener('click', function (e) {
                    e.stopPropagation();
                });
            }
        });
    } else {
        console.error('UL element not found using the provided XPath expression.');
    }
}
// Make the function accessible globally
window.activateDesignBeta = activateDesignBeta;</script>
