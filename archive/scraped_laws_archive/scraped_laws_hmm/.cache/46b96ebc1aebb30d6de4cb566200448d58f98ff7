            <script language="JavaScript">
                function printPopup() {
                    var lawCode = 'CIV';
                    var sectionNum = '1714.';
                    var op_statues = '2011'
                    var op_chapter = '410';
                    var op_section = '1';
                    var article = '';
                    var url = "printCodeSectionWindow.xhtml?";
                    if (lawCode)
                        url += 'lawCode=' + lawCode;
                    if (article) {
                        url += '&article=' + article;
                        //alert(url);
                    }

                    if (sectionNum)
                        url += '&sectionNum=' + sectionNum;
                    if (op_statues)
                        url += '&op_statues=' + op_statues;
                    if (op_chapter)
                        url += '&op_chapter=' + op_chapter;
                    if (op_section)
                        url += '&op_section=' + op_section;
                    printWindow = window.open(url, "wind", "width=680," +
                            "height=768,		" +
                            "toolbar=no,		" +
                            "location=no,		" +
                            "directories=no,	" +
                            "status=no,		" +
                            "menubar=yes,		" +
                            "scrollbars=yes,	" +
                            "resizable=yes		"
                            );
                    printWindow.focus();
                }
            </script>
