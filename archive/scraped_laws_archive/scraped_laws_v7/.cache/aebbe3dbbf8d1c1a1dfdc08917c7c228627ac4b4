	<script type="module">
		globalThis.tryPrint = () => {
			var contentWindow = document.getElementById("IFrame_StatuteText").contentWindow;
			var result = contentWindow.document.execCommand('print', false, null);
			if(!result)  contentWindow.print();
		}

		jQuery(document).ready(function() {
			jQuery( "#btn-textsize-normal" ).bind( "click", function() {
				textZoom('normal');
			});

			jQuery( "#btn-textsize-medium" ).bind( "click", function() {
				textZoom('medium');
			});

			jQuery( "#btn-textsize-large" ).bind( "click", function() {
				textZoom('large');
			});
		});

		globalThis.g_lineHeightRemoved = false;

		function textZoom(size) {
			if(!globalThis.g_lineHeightRemoved) {
				globalThis.g_lineHeightRemoved = true;

				var nodes = jQuery('#IFrame_StatuteText').contents().find('p');

				jQuery.each(nodes, function(index,element) {
					jQuery(element).css('line-height','normal');
				});
			}

			switch (size) {
				case "normal":
					jQuery('#IFrame_StatuteText').contents().find('body').css('font-size','');
				break;

				case "medium":
					jQuery('#IFrame_StatuteText').contents().find('body').css('font-size', '1.2em');
				break;

				case "large":
					jQuery('#IFrame_StatuteText').contents().find('body').css('font-size', '1.3em');
				break;
			}

			reAdjustIframeHeight();
		}

		
		globalThis.loadIframe = (queryParams) => {
			jQuery("#IFrame_StatuteText").on('load', function() {
				console.log('iFrame loaded!');
				globalThis.adjustHeight();

				// load in parent window when linking from within documnent so we don't display header and footer twice
				jQuery(this).contents().find("body a").each((index, element) => {
					element.target = '_top';

					const oldConsCheck = '/cfdocs/legis/LI/consCheck.cfm';
					const newConsCheck = '/statutes/consolidated/view-statute';
					element.href = element.href.replace(oldConsCheck, newConsCheck).replace(' ', '');

					const oldUnconsCheck = '/cfdocs/legis/LI/uconsCheck.cfm';
					const newUnconsCheck = '/statutes/unconsolidated/law-information/view-statute';
					element.href = element.href.replace(oldUnconsCheck, newUnconsCheck).replace(' ', '');
				});

			}).attr("src", '/statutes/consolidated/view-statute?57&iFrame=true' + queryParams);
		}

		globalThis.adjustHeight = () => {
			console.log('Adjusting height');

			const iFrameBody = jQuery('#IFrame_StatuteText').contents().find("body .BodyContainer");
			const contentsHeight = iFrameBody.height() + 75;
			const contentsWidth  = iFrameBody.width();

			var viewportWidth = jQuery('#IFrame_StatuteText').width();
			try {
				viewportWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;

			} catch (error) {
				console.error('Error getting viewport width: ', error);
			}

			if (contentsWidth > viewportWidth) {
				// This will expand the iFrame element so the content is not truncated.
				jQuery('#IFrame_StatuteText').css('width', `${contentsWidth}px`);
				iFrameBody.css('width', `${viewportWidth}px`);
			}

			if (contentsHeight > 300) {
				const bottomButton = document.getElementById('bottomButton');
				if (bottomButton) bottomButton.style.display = 'flex';
			}

			console.log('content : ', contentsHeight);
			jQuery('#IFrame_StatuteText').css('height', `${contentsHeight}px`);
		}
	</script>

	<style>
	</style>




