                function printPopup() {
                    var lawCode = 'BPC';
                    var sectionNum = '7159.5.';
                    var op_statues = '2023'
                    var op_chapter = '403';
                    var op_section = '1';
                    var article = '10.';
                    var url = "printCodeSectionWindow.xhtml?";
                    if (lawCode)
                        url += 'lawCode=' + lawCode;
                    if (article) {
                        url += '&article=' + article;
                        //alert(url);
                    }

                    if (sectionNum)
                        url += '&sectionNum=' + sectionNum;
                    if (op_statues)
                        url += '&op_statues=' + op_statues;
                    if (op_chapter)
                        url += '&op_chapter=' + op_chapter;
                    if (op_section)
                        url += '&op_section=' + op_section;
                    printWindow = window.open(url, "wind", "width=680," +
                            "height=768,		" +
                            "toolbar=no,		" +
                            "location=no,		" +
                            "directories=no,	" +
                            "status=no,		" +
                            "menubar=yes,		" +
                            "scrollbars=yes,	" +
                            "resizable=yes		"
                            );
                    printWindow.focus();
                }
            </script>
