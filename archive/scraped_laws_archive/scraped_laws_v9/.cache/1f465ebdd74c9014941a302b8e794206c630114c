        function logout() {
            var now = new Date();
            SetCookie("CheckIPDate", now.getFullYear() + "-" + now.getMonth() + "-" + now.getDate() + " " + now.getHours() + ":" + now.getMinutes() + ":" + now.getSeconds());
            location.reload(true);
            CheckLogout();
        }
