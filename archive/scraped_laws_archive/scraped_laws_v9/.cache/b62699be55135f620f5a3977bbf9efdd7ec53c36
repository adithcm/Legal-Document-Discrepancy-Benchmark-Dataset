<script charset="utf-8">function activateDesignBeta() {
    // Create a new <style> element
  const style = document.createElement('style');
  style.type = 'text/css';

  // Define the CSS styles
  const css = `
    #HEADER {
      height: 100px;
    }
    #DesktopLogoDiv > a > img {
      height: 50px;
      width: auto;
    }
    /* Search button */
    #SearchControl_WholeSearch {
        /* margin-top: 100px; */
      margin-top: 30px;
      width: 90%;
      margin-bottom: 10px;
    }
    #searchColumn {
      border-radius: 90px;
      margin-right: -60px;
      max-width: 700px !important;
    }
    #SearchControl_SearchTerm {
      border-bottom-left-radius: 16px !important;
      border-top-left-radius: 16px !important;
    }
    #SearchControl_SearchButton {
      border: 1px solid #012D69;
      border-top-right-radius: 16px !important;
      border-bottom-right-radius: 16px !important;
    }
    .dropdown-menu {
      border: .5px solid #012D69;
      border-radius: 5px !important;
    }
    .form-control {
      border-radius: 100%;
    }
    .navbar {
      margin-bottom: 100px;
    }
    .nav.navbar-nav.ae-exclude {}
    /* Study committees */
    .row.rowContainer.community-list {
      border-width: 6px 1px 1px;
      border-color: #012D69 #ccc #ccc;
      border-radius: 5px;
      display: flex;
      padding-left: 30px;
      flex-direction: row;
      align-items: center;
      padding-right: 0px;
    }
    .row.rowContainer.community-list>:nth-child(2) > div > div {
      padding: 0px;
    }
    .row.rowContainer.community-list>:nth-child(2) > div > div > div {
      display: flex;
      margin-right: 0px;
      flex-direction: column;
      justify-content: right;
    }
    .row.rowContainer.community-list>:nth-child(2) > div > div > div > a {
      margin-right: 0px;
      font-size: 18px;
    }
    /* Upcoming Events */
    .ContentUserControl.featured > .HLLandingControl.HLEventList > div.Content > div.row > div > ul > li {
      height: auto;
      padding: 16px;
      box-sizing: border-box;
      background-color: rgb(255,
        255,
        255);
      border-radius: 5px;
      border: 1px solid rgb(230,
        230,
        230);
      margin-bottom: 5px;
      display: block;
    }
    .ContentUserControl.featured > .HLLandingControl.HLEventList > div.Content > div.row > div > ul > li:hover {
      cursor: pointer;
      box-shadow: 0px 4px 16px 0px rgba(66,
        66,
        66,.2);
    }
  `;

  // Set the CSS styles as the content of the <style> element
  style.textContent = css;

  // Append the <style> element to the <head> of the document
    document.head.appendChild(style);
    AddClickEventListenerToUpcomingEvents()
}

function AddClickEventListenerToUpcomingEvents(){
     var ulElement =
        document.querySelector("div.ContentUserControl.featured> .HLLandingControl.HLEventList > div.Content > div.row >div > ul")
    console.log('ulElement', ulElement)
    // Evaluate the XPath expression to get the ul element
    // var ulElement = document.evaluate(xpathExpression, document, null, XPathResult.FIRST_ORDERED_NODE_TYPE, null).singleNodeValue;

    if (ulElement) {
        // Get all li elements within the ul group
        var liElements = ulElement.querySelectorAll('li');

        // Attach the click event listener to each li element
        liElements.forEach(function (li) {
            var link = li.querySelector('a') || li.querySelector('div > div > h3 > a');
            if (link) {
                li.addEventListener('click', function () {
                    link.click();
                });

                // Prevent the event from propagating to nested elements
                link.addEventListener('click', function (e) {
                    e.stopPropagation();
                });
            }
        });
    } else {
        console.error('UL element not found using the provided XPath expression.');
    }
}
// Make the function accessible globally
window.activateDesignBeta = activateDesignBeta;</script>
