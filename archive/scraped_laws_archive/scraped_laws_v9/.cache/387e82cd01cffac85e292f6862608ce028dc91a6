<!-- documentid:15_1129  usckey:150000000112900000000000000000000 currentthrough:20250129_119-1 documentPDFPage:-1 -->
<!-- itempath:/150/CHAPTER 22/SUBCHAPTER III/Sec. 1129 -->
<!-- itemsortkey:150ABSU -->
<!-- expcite:TITLE 15-COMMERCE AND TRADE!@!<usCode chapter="22" edition="prelim" title="15">CHAPTER 22</usCode>-TRADEMARKS!@!SUBCHAPTER III-GENERAL PROVISIONS!@!Sec. 1129 -->
<!-- field-start:head -->
<h3 class="section-head">&sect;1129. Transferred</h3>
<!-- field-end:head -->
<!-- field-start:statute -->
<br class="Q04" />
<!-- field-end:statute -->
<!-- field-start:notes -->
<!-- field-start:miscellaneous-note -->
<h4 class="note-head"><strong>Editorial Notes</strong></h4>
<!-- field-end:miscellaneous-note -->
<!-- field-start:codification-note -->
<h4 class="note-head">Codification</h4>
<p class="note-body">Section, <a href="/statviewer.htm?volume=113&amp;page=1536" target="_blank" onmouseout="this.style.textDecoration='none';" onmouseover="this.style.textDecoration='underline';" style="text-decoration: none;color: #0F0D61;"><pLaw congress="106" isPrivate="false" number="113">Pub. L. 106&ndash;113,</pLaw> div. B, &sect;1000(a)(9) [title III, &sect;3002(b)], Nov. 29, 1999, <statuteAtLarge page="1536" volume="113">113 Stat. 1536</statuteAtLarge></a>, <a href="/statviewer.htm?volume=113&amp;page=1501A-548" target="_blank" onmouseout="this.style.textDecoration='none';" onmouseover="this.style.textDecoration='underline';" style="text-decoration: none;color: #0F0D61;">1501A-548</a>, which related to cyberpiracy protections for individuals, was transferred to <usCode edition="prelim" section="8131" title="15">section 8131 of this title</usCode>.</p>
<!-- field-end:codification-note -->
