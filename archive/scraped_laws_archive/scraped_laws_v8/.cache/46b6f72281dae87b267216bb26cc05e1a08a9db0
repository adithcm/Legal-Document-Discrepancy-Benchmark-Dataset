        function doZoomSize(size) {
            var eTd = document.getElementById('e_font');
            var elists = eTd.getElementsByTagName("td");
            for (var i = 0; i < elists.length; i++) {
                var eTdnum = elists[i];
                eTdnum.style.fontSize = size + 'px';
            }
        }
